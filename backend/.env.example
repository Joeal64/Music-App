# Copy this file to .env and fill in your actual API keys
# DO NOT commit .env file to version control

# ACRCloud Configuration (Get from acrcloud.com)
ACR_HOST=your_acrcloud_host_here
ACR_ACCESS_KEY=your_acrcloud_access_key_here
ACR_ACCESS_SECRET=your_acrcloud_access_secret_here

# Last.fm Configuration (Get from last.fm/api)
LASTFM_ID=your_lastfm_api_key_here
LASTFM_SECRET=your_lastfm_secret_here
LASTFM_CLIENT_SECRET=your_lastfm_shared_secret_here
